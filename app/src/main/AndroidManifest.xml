<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.sudoku">

    <uses-permission
        android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"
        tools:ignore="ProtectedPermissions" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:anyDensity="true"
        android:icon="@drawable/sudokugame_launcher_full"
        android:label="@string/app_name"
        android:roundIcon="@drawable/sudokugame_launcher_full"
        android:supportsRtl="true"
        android:theme="@style/day_mode">
        <activity android:name="sudokugame.example.sudoku.record_of_sudoku" />
        <activity android:name="sudokugame.example.sudoku.settings_sudoku" />
        <activity android:name="sudokugame.example.sudoku.teaching_sudoku" />
        <activity android:name="sudokugame.example.sudoku.sudoku_play_easy" />
        <activity android:name="sudokugame.example.sudoku.sudoku_play_medium" />
        <activity android:name="sudokugame.example.sudoku.sudoku_play_hard" />
        <activity android:name="sudokugame.example.sudoku.select_level" />
        <activity android:name="sudokugame.example.sudoku.home_page">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>